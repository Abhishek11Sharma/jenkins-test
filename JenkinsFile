pipeline{
    agent any
    stages{
        stage('compile'){
            steps{
                 sh 'javac Sams.java'
            }
        }
        stage('run'){
           steps{
              sh 'java Sams'
           }
        }
    }

    post{
        always{
            sh ' echo "printning message" '
        }
        success{
            sh 'echo "success" '
        }

        failure{
            sh 'echo "failure" '
        }
    }
}